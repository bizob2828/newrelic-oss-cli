{
    "name": "oss-cli",
    "description": "Open Source Software CLI",
    "version": "0.0.1",
    "author": "devfreddy",
    "bin": {
        "oss": "./bin/run"
    },
    "main": "src/index.js",
    "oclif": {
        "commands": "./src/commands",
        "bin": "oss",
        "dirname": "oss-cli",
        "plugins": [
            "oclif-plugin-github",
            "@oclif/plugin-not-found",
            "@oclif/plugin-plugins",
            "@oclif/plugin-update",
            "@oclif/plugin-help",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-which",
            "@oclif/plugin-commands",
            "@oclif/plugin-autocomplete"
        ]
    },
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint .",
        "prepack": "oclif-dev manifest && oclif-dev readme",
        "test": "echo NO TESTS",
        "version": "oclif-dev readme && git add README.md"
    },
    "bugs": "https://github.com/newrelic/oss-cli/issues",
    "dependencies": {
        "@oclif/command": "^1.5.15",
        "@oclif/config": "^1.13.0",
        "oclif-plugin-github": "file:../oclif-plugin-github",
        "@oclif/plugin-not-found": "^1.2.2",
        "@oclif/plugin-plugins": "^1.7.8",
        "@oclif/plugin-update": "^1.3.9",
        "@oclif/plugin-help": "^2.2.0",
        "@oclif/plugin-warn-if-update-available": "^1.7.0",
        "@oclif/plugin-which": "^1.0.3",
        "@oclif/plugin-commands": "^1.2.3",
        "@oclif/plugin-autocomplete": "^0.1.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "1.22.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "files": [
        "/bin",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/src"
    ],
    "homepage": "https://github.com/newrelic/oss-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "repository": "newrelic/oss-cli"
}
